# Importação de biblioteca 
import threading  
import time    

# Estrutura de Thread 
def estruturathread (nome, inicio, fim):  # Função
    for i in range(inicio, fim +1): # Estrutura de repeteção 
        print(f"{nome} - > {i}")
        time.sleep(0.5) # Pausa a thread por 0.5 segundos entre cada numero
        
        # Criamos as threads 1 e 2 dizendo qual função cada uma vai executar o nome o inicio e o fim 
thread1 = threading.Thread(target=estruturathread,args=("thread1", 1, 10))        
thread2 = threading.Thread(target=estruturathread, args=("thread2", 50, 60))

# Iniciamos a execução das threads em paralelo
thread1.start()
thread2.start()

# Faz o programa principal esperar até que cada thread termine
thread1.join()
thread2.join()

print('Threands finalizadas com sucesso!')